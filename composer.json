{
    "name": "kiboko/oroneo-fork",
    "description": "OroCommerce Akeneo connector",
    "homepage": "https://github.com/synolia/oroneo",
    "license": "OSL-3.0",
    "authors": [{
        "name": "Synolia",
        "homepage": "http://www.synolia.com"
    }],
    "autoload": {
        "psr-4": {
            "Synolia\\Bundle\\OroneoBundle\\": ""
        }
    },
    "require": {
        "oro/commerce": "1.0.0-beta.5",
        "oro/platform-serialised-fields": "1.5.0-beta.4"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.11",
        "escapestudios/symfony2-coding-standard": "~2.0",
        "symfony/phpunit-bridge": "~3.0",
        "phpunit/phpunit": "4.8.*",
        "johnkary/phpunit-speedtrap": "1.0.*",
        "mybuilder/phpunit-accelerator": "1.1.*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
